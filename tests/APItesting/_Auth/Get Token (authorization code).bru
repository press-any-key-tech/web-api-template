meta {
  name: Get Token (authorization code)
  type: http
  seq: 2
}

post {
  url: {{id_provider_domain}}/oauth2/token
  body: formUrlEncoded
  auth: none
}

body:form-urlencoded {
  client_id: {{client_id_auth_code}}
